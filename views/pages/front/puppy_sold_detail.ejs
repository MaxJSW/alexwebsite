<!DOCTYPE html>
<html lang="fr">
    <head>
        <meta charset="UTF-8" />
        <meta name="viewport" content="width=device-width, initial-scale=1.0" />
        <meta http-equiv="X-UA-Compatible" content="IE=edge" />
        <title><%= puppy.name %> - Chiot <%= puppy.breed.name %><% if (puppy.breed.type) { %> <%= puppy.breed.type %><% } %> <%= puppy.gender === 'Mâle' ? 'Mâle' : 'Femelle' %> Produits</title>
        <meta name="description" content="<%= puppy.name %> : chiot <%= puppy.breed.name %><% if (puppy.breed.type) { %> <%= puppy.breed.type %><% } %> <%= puppy.gender === 'Mâle' ? 'mâle' : 'femelle' %> Produits <% if (puppy.color) { %>, robe <%= puppy.color.toLowerCase() %><% } %>. Production de l'Élevage de Bel Air proche de Montpellier - Occitanie." />
        <meta name="keywords" content="<%= puppy.name %>, <%= puppy.breed.name %> Produits , chiot <%= puppy.breed.name %>, <%= puppy.gender === 'Mâle' ? 'mâle' : 'femelle' %><% if (puppy.color) { %>, <%= puppy.color.toLowerCase() %><% } %>, élevage <%= puppy.breed.name %>, production <%= puppy.breed.name %>, <%= puppy.breed.name %> france, élevage de bel air"/>
        <meta name="author" content="Élevage de Bel Air" />
        <meta name="robots" content="index, follow, max-snippet:-1, max-image-preview:large, max-video-preview:-1" />
        <meta name="googlebot" content="index, follow" />
        <link rel="canonical" href="https://elevagedebelair.com/chiots/produits/<%= puppy.breed.slug %>/<%= puppy.slug %>">
        <meta name="language" content="fr" />
        <meta name="geo.region" content="FR-OCC" />
        <meta name="geo.placename" content="Occitanie, France" />
        <meta property="og:type" content="article" />
        <meta property="og:site_name" content="Élevage de Bel Air" />
        <meta property="og:title" content="<%= puppy.name %> - Chiot <%= puppy.breed.name %> <%= puppy.gender === 'Mâle' ? 'Mâle' : 'Femelle' %> Produits " />
        <meta property="og:description" content="<%= puppy.name %> : chiot <%= puppy.breed.name %> Produits  <%= puppy.gender === 'Mâle' ? 'mâle' : 'femelle' %> issu de notre élevage familial en Occitanie." />
        <meta property="og:url" content="https://elevagedebelair.com/chiots/produits/<%= puppy.breed.slug %>/<%= puppy.slug %>" />
        <meta property="og:image" content="https://elevagedebelair.com<%= ogImage || '/img/og/og-image.webp' %>">
        <meta property="og:image:width" content="1200" />
        <meta property="og:image:height" content="630" />
        <meta property="og:image:alt" content="<%= puppy.name %> - Chiot <%= puppy.breed.name %> - Élevage de Bel Air" />
        <meta property="og:locale" content="fr_FR" />
        <meta name="twitter:card" content="summary_large_image" />
        <meta name="twitter:title" content="<%= puppy.name %> - Chiot <%= puppy.breed.name %> <%= puppy.gender === 'Mâle' ? 'Mâle' : 'Femelle' %> Produits " />
        <meta name="twitter:description" content="<%= puppy.name %> : chiot <%= puppy.breed.name %> Produits  issu de l'Élevage de Bel Air." />
        <meta name="twitter:image" content="https://elevagedebelair.com<%= ogImage || '/img/og/og-image.webp' %>">
        <meta name="twitter:image:alt" content="<%= puppy.name %> - Élevage de Bel Air" />
        <link rel="icon" type="image/x-icon" href="/favicon.ico" />
        <link rel="icon" type="image/png" sizes="16x16" href="/images/favicon-16x16.png" />
        <link rel="icon" type="image/png" sizes="32x32" href="/images/favicon-32x32.png" />
        <link rel="apple-touch-icon" sizes="180x180" href="/images/apple-touch-icon.png" />
        <link rel="manifest" href="/site.webmanifest" />
        <meta name="theme-color" content="#65C178" />
        <meta name="msapplication-TileColor" content="#65C178" />
        <link rel="preconnect" href="https://fonts.googleapis.com" />
        <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin />
        <link href="https://fonts.googleapis.com/css2?family=Montserrat:wght@300;400;500;600;700;800&family=Playfair+Display:wght@400;500;600;700&display=swap" rel="stylesheet" />
        <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-QWTKZyjpPEjISv5WaRU9OFeRpok6YctnYmDr5pNlyT2bRjXh0JMhjY6hW+ALEwIH" crossorigin="anonymous" />
        <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/swiper@11/swiper-bundle.min.css" />
        <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/@fancyapps/ui@5.0/dist/fancybox/fancybox.css" />
        <link href="https://cdn.jsdelivr.net/npm/aos@2.3.4/dist/aos.css" rel="stylesheet" />
        <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.5.1/css/all.min.css" integrity="sha512-DTOQO9RWCH3ppGqcWaEA1BIZOC6xxalwEsw9c2QQeAIftl+Vegovlnee1c9QX4TctnWMn13TZye+giMm8e2LwA==" crossorigin="anonymous" referrerpolicy="no-referrer"/>
        <link rel="stylesheet" href="/front/css/navbar.css" />
        <link rel="stylesheet" href="/front/css/puppy-details.css" />
        <link rel="preload" href="https://fonts.googleapis.com/css2?family=Montserrat:wght@300;400;500;600;700;800&family=Playfair+Display:wght@400;500;600;700&display=swap" as="style" />
        <link rel="dns-prefetch" href="https://fonts.googleapis.com" />
        <link rel="dns-prefetch" href="https://fonts.gstatic.com" />
        <link rel="dns-prefetch" href="https://cdn.jsdelivr.net" />
    </head>
    <body class="h-100">
        <%- include('../front/frontEndAssets/header') %>
        <main style="padding-top: 80px;">
            <section class="puppy-detail-hero">
                <div class="container">
                    <div class="row g-4">
                        <div class="col-lg-6" data-aos="fade-right">
                            <div class="puppy-gallery-container">
                                <div class="puppy-status-badge <%= puppy.status === 'À vendre' ? 'available' : puppy.status === 'Disponible à la réservation' ? 'reservation' : 'reserved' %>">
                                    <%= puppy.status %>
                                </div>
                                <div class="carousel-wrapper">
                                    <div class="swiper mainPuppySwiper">
                                        <div class="swiper-wrapper">
                                            <% images.forEach((image, index) => { %>
                                            <div class="swiper-slide">
                                                <a href="<%= image.path %>" data-fancybox="puppy-gallery" data-caption="<%= puppy.name %> - Photo <%= index + 1 %>">
                                                    <div class="puppy-carousel-image">
                                                        <img src="<%= image.path %>" alt="<%= image.alt %>" onerror="this.onerror=null; this.src='/img/placeholder-puppy.jpg';" />
                                                        <div class="puppy-carousel-overlay">
                                                            <i class="fas fa-search-plus"></i>
                                                        </div>
                                                    </div>
                                                </a>
                                            </div>
                                            <% }); %>
                                        </div>
                                        <div class="swiper-button-next puppy-btn-next"></div>
                                        <div class="swiper-button-prev puppy-btn-prev"></div>
                                        <div class="puppy-carousel-counter">
                                            <i class="fas fa-camera"></i>
                                            <span class="counter-current">1</span> / <span class="counter-total"><%= images.length %></span>
                                        </div>
                                    </div>
                                </div>
                                <div class="puppy-thumbnails">
                                    <% images.slice(0, 6).forEach((image, index) => { %>
                                    <div class="thumbnail-item" data-index="<%= index %>">
                                        <img src="<%= image.path %>" alt="<%= image.alt %>" onerror="this.onerror=null; this.src='/img/placeholder-puppy.jpg';" />
                                    </div>
                                    <% }); %>
                                </div>
                            </div>
                        </div>
                        <div class="col-lg-6" data-aos="fade-left">
                            <div class="puppy-info-container">
                                <div class="puppy-header">
                                    <div class="puppy-name-section">
                                        <h1 class="puppy-name"><%= puppy.name %></h1>
                                        <div class="puppy-gender <%= puppy.gender === 'Mâle' ? 'male' : 'female' %>">
                                            <i class="fas <%= puppy.gender === 'Mâle' ? 'fa-mars' : 'fa-venus' %>"></i>
                                            <%= puppy.gender === 'Mâle' ? 'Mâle' : 'Femelle' %>
                                        </div>
                                    </div>
                                </div>
                                <div class="puppy-main-info">
                                    <div class="info-row">
                                        <div class="info-label">
                                            <i class="fas fa-paw"></i>
                                            Race
                                        </div>
                                        <div class="info-value">
                                            <a href="/chiots/a-vendre/<%= puppy.breed.slug %>">
                                                <%= puppy.breed.name %>
                                            </a>
                                            <% if (puppy.breed.type) { %>
                                            <span class="type-badge"><%= puppy.breed.type %></span>
                                            <% } %>
                                        </div>
                                    </div>
                                    <div class="info-row">
                                        <div class="info-label">
                                            <i class="fas fa-calendar-alt"></i>
                                            Âge
                                        </div>
                                        <div class="info-value">
                                            <%= puppy.age %> mois
                                        </div>
                                    </div>
                                    <% if (puppy.color) { %>
                                    <div class="info-row">
                                        <div class="info-label">
                                            <i class="fas fa-palette"></i>
                                            Couleur
                                        </div>
                                        <div class="info-value">
                                            <%= puppy.color %>
                                        </div>
                                    </div>
                                    <% } %> <% if (puppy.eyeColor) { %>
                                    <div class="info-row">
                                        <div class="info-label">
                                            <i class="fas fa-eye"></i>
                                            Yeux
                                        </div>
                                        <div class="info-value">
                                            <%= puppy.eyeColor %>
                                        </div>
                                    </div>
                                    <% } %> <% if (puppy.register && puppy.register.name) { %>
                                    <div class="info-row">
                                        <div class="info-label">
                                            <i class="fas fa-eye"></i>
                                            Registre
                                        </div>
                                        <div class="info-value">
                                            <%= puppy.register.name %>
                                        </div>
                                    </div>
                                    <% } %> <% if (puppy.coatType) { %>
                                    <div class="info-row">
                                        <div class="info-label">
                                            <i class="fas fa-cut"></i>
                                            Poil
                                        </div>
                                        <div class="info-value">
                                            <%= puppy.coatType %>
                                        </div>
                                    </div>
                                    <% } %> <% if (puppy.country) { %>
                                    <div class="info-row">
                                        <div class="info-label">
                                            <i class="fas fa-eye"></i>
                                            Origine
                                        </div>
                                        <div class="info-value">
                                            <%= puppy.country %>
                                        </div>
                                    </div>
                                    <% } %> <% if (puppy.availableDate) { %>
                                    <div class="info-row highlight">
                                        <div class="info-label">
                                            <i class="fas fa-clock"></i>
                                           Était disponible le
                                        </div>
                                        <div class="info-value">
                                            <%= puppy.availableDate %>
                                        </div>
                                    </div>
                                    <% } %>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </section>
            <% if (puppy.description) { %>
            <section class="puppy-description-section">
                <div class="container">
                    <div class="puppy-description-card" data-aos="fade-up">
                        <h2 class="section-title">
                            <i class="fas fa-align-left"></i>
                            À propos de <%= puppy.name %>
                        </h2>

                        <div class="description-content">
                            <%- puppy.description %>
                        </div>
                    </div>
                </div>
            </section>
            <% } %> <% if (father || mother) { %>
            <section class="puppy-pedigree-section">
                <div class="container">
                    <div class="pedigree-header" data-aos="fade-up">
                        <h2 class="pedigree-title">
                            <i class="fas fa-sitemap"></i>
                            Arbre Généalogique
                        </h2>
                        <p class="pedigree-subtitle">Découvrez les origines de <%= puppy.name %></p>
                    </div>

                    <div class="pedigree-tree">
                        <div class="pedigree-level level-1" data-aos="fade-up" data-aos-delay="100">
                            <div class="pedigree-card <%= father ? '' : 'empty' %>">
                                <% if (father) { %>
                                <div class="pedigree-card-content">
                                    <% if (father.image && father.image.path) { %>
                                    <div class="pedigree-image">
                                        <img src="<%= father.image.path %>" alt="<%= father.name %>" onerror="this.onerror=null; this.src='/img/placeholder-dog.jpg';" />
                                    </div>
                                    <% } else { %>
                                    <div class="pedigree-badge male">
                                        <i class="fas fa-mars"></i>
                                    </div>
                                    <% } %>
                                    <div class="pedigree-info">
                                        <span class="pedigree-label">Père</span>
                                        <span class="pedigree-name"><%= father.name %></span>
                                        <% if (father.breed && father.breed.type) { %>
                                        <span class="pedigree-breed"><%= father.breed.type %></span>
                                        <% } %>
                                    </div>
                                </div>
                                <% } else { %>
                                <div class="pedigree-card-content">
                                    <span class="pedigree-unknown">Inconnu</span>
                                </div>
                                <% } %>
                            </div>
                            <div class="pedigree-card <%= mother ? '' : 'empty' %>">
                                <% if (mother) { %>
                                <div class="pedigree-card-content">
                                    <% if (mother.image && mother.image.path) { %>
                                    <div class="pedigree-image">
                                        <img src="<%= mother.image.path %>" alt="<%= mother.name %>" onerror="this.onerror=null; this.src='/img/placeholder-dog.jpg';" />
                                    </div>
                                    <% } else { %>
                                    <div class="pedigree-badge female">
                                        <i class="fas fa-venus"></i>
                                    </div>
                                    <% } %>
                                    <div class="pedigree-info">
                                        <span class="pedigree-label">Mère</span>
                                        <span class="pedigree-name"><%= mother.name %></span>
                                        <% if (mother.breed && mother.breed.type) { %>
                                        <span class="pedigree-breed"><%= mother.breed.type %></span>
                                        <% } %>
                                    </div>
                                </div>
                                <% } else { %>
                                <div class="pedigree-card-content">
                                    <span class="pedigree-unknown">Inconnue</span>
                                </div>
                                <% } %>
                            </div>
                        </div>
                        <div class="pedigree-level level-2" data-aos="fade-up" data-aos-delay="200">
                            <div class="pedigree-card <%= grandparents && grandparents.grandfather ? '' : 'empty' %>">
                                <% if (grandparents && grandparents.grandfather) { %>
                                <div class="pedigree-card-content">
                                    <% if (grandparents.grandfather.image && grandparents.grandfather.image.path) { %>
                                    <div class="pedigree-image small">
                                        <img src="<%= grandparents.grandfather.image.path %>" alt="<%= grandparents.grandfather.name %>" onerror="this.onerror=null; this.src='/img/placeholder-dog.jpg';" />
                                    </div>
                                    <% } else { %>
                                    <div class="pedigree-badge male small">
                                        <i class="fas fa-mars"></i>
                                    </div>
                                    <% } %>
                                    <div class="pedigree-info">
                                        <span class="pedigree-label">Grand-père paternel</span>
                                        <span class="pedigree-name small"><%= grandparents.grandfather.name %></span>
                                    </div>
                                </div>
                                <% } else { %>
                                <div class="pedigree-card-content">
                                    <span class="pedigree-unknown">Inconnu</span>
                                </div>
                                <% } %>
                            </div>
                            <div class="pedigree-card <%= grandparents && grandparents.grandmother ? '' : 'empty' %>">
                                <% if (grandparents && grandparents.grandmother) { %>
                                <div class="pedigree-card-content">
                                    <% if (grandparents.grandmother.image && grandparents.grandmother.image.path) { %>
                                    <div class="pedigree-image small">
                                        <img src="<%= grandparents.grandmother.image.path %>" alt="<%= grandparents.grandmother.name %>" onerror="this.onerror=null; this.src='/img/placeholder-dog.jpg';" />
                                    </div>
                                    <% } else { %>
                                    <div class="pedigree-badge female small">
                                        <i class="fas fa-venus"></i>
                                    </div>
                                    <% } %>
                                    <div class="pedigree-info">
                                        <span class="pedigree-label">Grand-mère paternelle</span>
                                        <span class="pedigree-name small"><%= grandparents.grandmother.name %></span>
                                    </div>
                                </div>
                                <% } else { %>
                                <div class="pedigree-card-content">
                                    <span class="pedigree-unknown">Inconnue</span>
                                </div>
                                <% } %>
                            </div>
                            <div class="pedigree-card <%= maternal_grandparents && maternal_grandparents.grandfather ? '' : 'empty' %>">
                                <% if (maternal_grandparents && maternal_grandparents.grandfather) { %>
                                <div class="pedigree-card-content">
                                    <% if (maternal_grandparents.grandfather.image && maternal_grandparents.grandfather.image.path) { %>
                                    <div class="pedigree-image small">
                                        <img src="<%= maternal_grandparents.grandfather.image.path %>" alt="<%= maternal_grandparents.grandfather.name %>" onerror="this.onerror=null; this.src='/img/placeholder-dog.jpg';" />
                                    </div>
                                    <% } else { %>
                                    <div class="pedigree-badge male small">
                                        <i class="fas fa-mars"></i>
                                    </div>
                                    <% } %>
                                    <div class="pedigree-info">
                                        <span class="pedigree-label">Grand-père maternel</span>
                                        <span class="pedigree-name small"><%= maternal_grandparents.grandfather.name %></span>
                                    </div>
                                </div>
                                <% } else { %>
                                <div class="pedigree-card-content">
                                    <span class="pedigree-unknown">Inconnu</span>
                                </div>
                                <% } %>
                            </div>
                            <div class="pedigree-card <%= maternal_grandparents && maternal_grandparents.grandmother ? '' : 'empty' %>">
                                <% if (maternal_grandparents && maternal_grandparents.grandmother) { %>
                                <div class="pedigree-card-content">
                                    <% if (maternal_grandparents.grandmother.image && maternal_grandparents.grandmother.image.path) { %>
                                    <div class="pedigree-image small">
                                        <img src="<%= maternal_grandparents.grandmother.image.path %>" alt="<%= maternal_grandparents.grandmother.name %>" onerror="this.onerror=null; this.src='/img/placeholder-dog.jpg';" />
                                    </div>
                                    <% } else { %>
                                    <div class="pedigree-badge female small">
                                        <i class="fas fa-venus"></i>
                                    </div>
                                    <% } %>
                                    <div class="pedigree-info">
                                        <span class="pedigree-label">Grand-mère maternelle</span>
                                        <span class="pedigree-name small"><%= maternal_grandparents.grandmother.name %></span>
                                    </div>
                                </div>
                                <% } else { %>
                                <div class="pedigree-card-content">
                                    <span class="pedigree-unknown">Inconnue</span>
                                </div>
                                <% } %>
                            </div>
                        </div>
                    </div>
                </div>
            </section>
            <% } %> <% if (siblings && siblings.length > 0) { %>
            <section class="puppy-siblings-section">
                <div class="container">
                    <div class="siblings-header" data-aos="fade-up">
                        <h2 class="section-title">
                            Les Frères et Sœurs de <%= puppy.name %>
                        </h2>
                        <p class="siblings-subtitle">
                            Découvrez les autres chiots de la même portée
                        </p>
                    </div>

                                        <div class="row g-4">
                        <% siblings.forEach((sibling, index) => { 
                            // Définir l'URL selon le statut
                            const profileUrl = sibling.status === 'sold' 
                                ? `/chiots/produits/${sibling.breed.slug}/${sibling.slug}`
                                : `/chiots/a-vendre/${sibling.breed.slug}/${sibling.slug}`;
                            
                            // Définir le texte du statut
                            let statusText = '';
                            let statusClass = '';
                            
                            if (sibling.status === 'for_sale') {
                                statusText = 'À vendre';
                                statusClass = 'available';
                            } else if (sibling.status === 'available_for_reservation') {
                                statusText = 'Disponible';
                                statusClass = 'available';
                            } else if (sibling.status === 'in_reservation') {
                                statusText = 'En réservation';
                                statusClass = 'reserved';
                            } else if (sibling.status === 'reserved') {
                                statusText = 'Réservé';
                                statusClass = 'reserved';
                            } else if (sibling.status === 'sold') {
                                statusText = 'Vendu';
                                statusClass = 'sold';
                            }
                        %>
                        <div class="col-lg-4 col-md-6" data-aos="fade-up" data-aos-delay="<%= index * 100 %>">
                            <article class="sibling-card">
                                <div class="sibling-image">
                                    <a href="<%= profileUrl %>">
                                        <img src="<%= sibling.image.path %>" 
                                            alt="<%= sibling.image.alt %>" 
                                            onerror="this.onerror=null; this.src='/img/placeholder-puppy.jpg';">
                                    </a>
                                    <!-- Badge Genre -->
                                    <div class="sibling-gender <%= sibling.gender === 'Mâle' ? 'male' : 'female' %>">
                                        <i class="fas <%= sibling.gender === 'Mâle' ? 'fa-mars' : 'fa-venus' %>"></i>
                                    </div>
                                    <!-- Badge Statut -->
                                    <div class="sibling-status <%= statusClass %>">
                                        <%= statusText %>
                                    </div>
                                </div>
                                <div class="sibling-body">
                                    <h3 class="sibling-name">
                                        <a href="<%= profileUrl %>">
                                            <%= sibling.name %>
                                        </a>
                                    </h3>
                                    <div class="sibling-info">
                                        <span class="sibling-breed">
                                            <i class="fas fa-paw"></i>
                                            <%= sibling.breed.name %>
                                        </span>
                                        <% if (sibling.color) { %>
                                        <span class="sibling-color">
                                            <i class="fas fa-palette"></i>
                                            <%= sibling.color %>
                                        </span>
                                        <% } %>
                                    </div>
                                    <a href="<%= profileUrl %>" class="btn-sibling">
                                        Voir le profil
                                        <i class="fas fa-arrow-right"></i>
                                    </a>
                                </div>
                            </article>
                        </div>
                        <% }); %>
                    </div>
                </div>
            </section>
            <% } %>
        </main>
        <%- include('../front/frontEndAssets/footer') %>
        <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/js/bootstrap.bundle.min.js" integrity="sha384-YvpcrYf0tY3lHB60NNkmXc5s9fDVZLESaAA55NDzOxhy9GkcIdslK1eN7N6jIeHz" crossorigin="anonymous"></script>
        <script src="https://cdn.jsdelivr.net/npm/swiper@11/swiper-bundle.min.js"></script>
        <script src="https://cdn.jsdelivr.net/npm/@fancyapps/ui@5.0/dist/fancybox/fancybox.umd.js"></script>
        <script src="/front/js/header.js"></script>
        <%- include('./partials/googleAnalytics') %>
        <script src="https://cdn.jsdelivr.net/npm/aos@2.3.4/dist/aos.js"></script>
        <script>
            AOS.init({
                duration: 1000,
                easing: "ease-in-out",
                once: true,
                offset: 120,
                delay: 0,
                anchorPlacement: "top-bottom",
                mirror: false,
                disable: "mobile",
            });
        </script>
        <script>
        document.addEventListener("DOMContentLoaded", function () {
            const mainPuppySwiper = new Swiper('.mainPuppySwiper', {
                spaceBetween: 0,
                loop: false,
                navigation: {
                    nextEl: '.puppy-btn-next',
                    prevEl: '.puppy-btn-prev',
                },
                on: {
                    slideChange: function () {
                        const currentCounter = document.querySelector('.counter-current');
                        if (currentCounter) {
                            currentCounter.textContent = this.activeIndex + 1;
                        }
                        document.querySelectorAll('.thumbnail-item').forEach(t => t.classList.remove('active'));
                        const activeThumb = document.querySelectorAll('.thumbnail-item')[this.activeIndex];
                        if (activeThumb) {
                            activeThumb.classList.add('active');
                        }
                    }
                },
                observer: true,
                observeParents: true,
            });
            document.querySelectorAll('.thumbnail-item').forEach((thumb, index) => {
                thumb.addEventListener('click', function() {
                    mainPuppySwiper.slideTo(index);
                });
            });
            if (document.querySelector('.thumbnail-item')) {
                document.querySelector('.thumbnail-item').classList.add('active');
            }
            Fancybox.bind('[data-fancybox="puppy-gallery"]', {
                Toolbar: {
                    display: {
                        left: [],
                        middle: [],
                        right: ["close"],
                    },
                },
                Images: {
                    zoom: true,
                },
                Carousel: {
                    infinite: true,
                },
            });
        });
        </script>
    </body>
</html>