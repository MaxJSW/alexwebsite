        <script>
            document.addEventListener("DOMContentLoaded", function () {
                Fancybox.bind('[data-fancybox="dog-gallery"]', {
                    Toolbar: {
                        display: {
                            left: [],
                            middle: [],
                            right: ["close"],
                        },
                    },
                    Images: {
                        zoom: true,
                    },
                    Carousel: {
                        infinite: true,
                    },
                });
                document.querySelectorAll('.dog-detail-thumb').forEach((thumb) => {
                    thumb.addEventListener('click', function () {
                        document.querySelectorAll('.dog-detail-thumb').forEach(t => t.classList.remove('active'));
                        this.classList.add('active');
                    });
                });
            });
        </script>
