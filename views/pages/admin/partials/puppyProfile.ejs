<div class="card widget-card border-light shadow-sm col-md-12 col-sm-12">
    <div class="card-body p-4 ">
        <ul class="nav nav-tabs" id="profileTab" role="tablist">
            <li class="nav-item" role="presentation">
                <button class="nav-link active" id="overview-tab" data-bs-toggle="tab" data-bs-target="#overview-tab-pane" type="button" role="tab" aria-controls="overview-tab-pane" aria-selected="true">Infos</button>
            </li>
            <li class="nav-item" role="presentation">
                <button class="nav-link" id="profile-tab" data-bs-toggle="tab" data-bs-target="#profile-tab-pane" type="button" role="tab" aria-controls="profile-tab-pane" aria-selected="false">Modifier</button>
            </li>
            <li class="nav-item" role="presentation">
                <button class="nav-link" id="email-tab" data-bs-toggle="tab" data-bs-target="#email-tab-pane" type="button" role="tab" aria-controls="email-tab-pane" aria-selected="false">Texte</button>
            </li>
            <li class="nav-item" role="presentation">
                <button class="nav-link" id="password-tab" data-bs-toggle="tab" data-bs-target="#password-tab-pane" type="button" role="tab" aria-controls="password-tab-pane" aria-selected="false">Photos</button>
            </li>
            <li class="nav-item" role="presentation">
                <button class="nav-link" id="description-tab" data-bs-toggle="tab" data-bs-target="#description-tab-pane" type="button" role="tab" aria-controls="description-tab-pane" aria-selected="false" >Description</button>
            </li>
        </ul>
        <div class="tab-content pt-4" id="profileTabContent">
            <div class="tab-pane fade show active" id="overview-tab-pane" role="tabpanel" aria-labelledby="overview-tab" tabindex="0">
                <h5 class="mb-3 h2 page-title">Profil de <%= puppy.puppy_name %></h5>
                <div class="row g-0">
                    <div class="col-5 col-md-3 bg-body-tertiary border-bottom border-white border-1">
                        <div class="p-2">Nom</div>
                    </div>
                    <div class="col-7 col-md-9 bg-body-tertiary border-start border-bottom border-white border-1">
                        <div class="p-2"><%= puppy.puppy_name %></div>
                    </div>
                    <div class="col-5 col-md-3 bg-body-tertiary border-bottom border-white border-1">
                        <div class="p-2">Nom Complet</div>
                    </div>
                    <div class="col-7 col-md-9 bg-body-tertiary border-start border-bottom border-white border-1">
                        <div class="p-2"><%= puppy.puppy_other_names %></div>
                    </div>
                    <div class="col-5 col-md-3 bg-body-tertiary border-bottom border-white border-1">
                        <div class="p-2">Catégorie</div>
                    </div>
                    <div class="col-7 col-md-9 bg-body-tertiary border-start border-bottom border-white border-1">
                        <div class="p-2"><%= puppy.puppy_category %></div>
                    </div>
                    <div class="col-5 col-md-3 bg-body-tertiary border-bottom border-white border-1">
                        <div class="p-2">Genre</div>
                    </div>
                    <div class="col-7 col-md-9 bg-body-tertiary border-start border-bottom border-white border-1">
                        <div class="p-2"><%= puppy.puppy_gender %></div>
                    </div>
                    <div class="col-5 col-md-3 bg-body-tertiary border-bottom border-white border-1">
                        <div class="p-2">Race</div>
                    </div>
                    <div class="col-7 col-md-9 bg-body-tertiary border-start border-bottom border-white border-1">
                        <div class="p-2"><%= puppy.breed_name %></div>
                    </div>
                    
                    <div class="col-5 col-md-3 bg-body-tertiary border-bottom border-white border-1">
                        <div class="p-2">Couleur</div>
                    </div>
                    <div class="col-7 col-md-9 bg-body-tertiary border-start border-bottom border-white border-1">
                        <div class="p-2"><%= puppy.puppy_color %></div>
                    </div>
                    <div class="col-5 col-md-3 bg-body-tertiary border-bottom border-white border-1">
                        <div class="p-2">Type de Pelage</div>
                    </div>
                    <div class="col-7 col-md-9 bg-body-tertiary border-start border-bottom border-white border-1">
                        <div class="p-2"><%= puppy.puppy_coat_type %></div>
                    </div>
                    <div class="col-5 col-md-3 bg-body-tertiary border-bottom border-white border-1">
                        <div class="p-2">Couleur des Yeux</div>
                    </div>
                    <div class="col-7 col-md-9 bg-body-tertiary border-start border-bottom border-white border-1">
                        <div class="p-2"><%= puppy.puppy_eye_color %></div>
                    </div>
                    <div class="col-5 col-md-3 bg-body-tertiary border-bottom border-white border-1">
                        <div class="p-2">Pays de Naissance</div>
                    </div>
                    <div class="col-7 col-md-9 bg-body-tertiary border-start border-bottom border-white border-1">
                        <div class="p-2"><%= puppy.country_name %></div>
                    </div>
                    <div class="col-5 col-md-3 bg-body-tertiary border-bottom border-white border-1">
                        <div class="p-2">Date de Naissance</div>
                    </div>
                    <div class="col-7 col-md-9 bg-body-tertiary border-start border-bottom border-white border-1">
                        <div class="p-2">
                            <%= new Date(puppy.puppy_birth_date).toLocaleDateString('fr-FR', { year: 'numeric', month: 'long', day: 'numeric' }) %>
                            (<% if (puppy.ageInMonths >= 1) { %>
                                <%= puppy.ageInMonths %> Mois
                            <% } else { %>
                                <%= puppy.ageInDays %> Jours
                            <% } %>)
                        </div>
                    </div>
                    <div class="col-5 col-md-3 bg-body-tertiary border-bottom border-white border-1">
                        <div class="p-2">Disponible le </div>
                    </div>
                    <div class="col-7 col-md-9 bg-body-tertiary border-start border-bottom border-white border-1">
                        <div class="p-2">
                            <%= new Date(puppy.available_date).toLocaleDateString('fr-FR', { year: 'numeric', month: 'long', day: 'numeric' }) %>
                        </div>
                    </div>

                    <div class="col-5 col-md-3 bg-body-tertiary border-bottom border-white border-1">
                        <div class="p-2">Statut</div>
                    </div>
                    <div class="col-7 col-md-9 bg-body-tertiary border-start border-bottom border-white border-1 <%= puppy.puppy_is_online ? 'status-online' : 'status-offline' %>">
                        <div class="p-2"><%= puppy.puppy_is_online ? 'En Ligne' : 'Hors-Ligne' %></div>
                    </div>

                    <div class="col-5 col-md-3 bg-body-tertiary border-bottom border-white border-1">
                        <div class="p-2">Prix</div>
                    </div>
                    <div class="col-7 col-md-9 bg-body-tertiary border-start border-bottom border-white border-1">
                        <div class="p-2">
                            <% if (puppy.price) { %>
                                <%= puppy.price %> €
                            <% } else { %>
                                Aucun prix défini
                            <% } %>
                        </div>
                    </div>
                    <div class="col-5 col-md-3 bg-body-tertiary border-bottom border-white border-1">
                        <div class="p-2">Fait partie de l'élevage</div>
                    </div>
                    <div class="col-7 col-md-9 bg-body-tertiary border-start border-bottom border-white border-1">
                        <div class="p-2"><%= puppy.in_breeding ? 'Oui' : 'Non' %></div>
                    </div>
                    <div class="col-5 col-md-3 bg-body-tertiary border-bottom border-white border-1">
                        <div class="p-2">Père</div>
                    </div>
                    <div class="col-7 col-md-9 bg-body-tertiary border-start border-bottom border-white border-1">
                        <div class="p-2"><%= puppy.father_name %></div>
                    </div>
                    <div class="col-5 col-md-3 bg-body-tertiary border-bottom border-white border-1">
                        <div class="p-2">Mère</div>
                    </div>
                    <div class="col-7 col-md-9 bg-body-tertiary border-start border-bottom border-white border-1">
                        <div class="p-2"><%= puppy.mother_name %></div>
                    </div>
                </div>
                
                <hr>
                <div class="container">
                    <h5 class="mt-3 h2 page-title" style="text-decoration:underline;">Test(s) de santé</h5>
                    <% if (puppy.puppy_health_tests && puppy.puppy_health_tests.trim() !== "") { %>
                        <p class="small mt-3"><%= puppy.puppy_health_tests %></p>
                    <% } else { %>
                        <p class="small mt-3">Aucun test de santé disponible</p>
                    <% } %>
                </div>
                <hr>
                <div class="container">
                    <h5 class="mb-3 h2 page-title" style="text-decoration:underline;">Description</h5>
                    <% if (puppy.puppy_description_animals && puppy.puppy_description_animals.trim() !== "") { %>
                        <p class="small mb-3"><%= puppy.puppy_description_animals %></p>
                    <% } else { %>
                        <p class="small mb-3">Aucune description disponible</p>
                    <% } %>
                </div>
            </div>
            <div class="tab-pane fade" id="profile-tab-pane" role="tabpanel" aria-labelledby="profile-tab" tabindex="0">
                <div class="row gy-3 gy-xxl-4 mb-3">
                    <div class="col-12">
                        <div class="row gy-2 p-3 border rounded mb-4">
                            <label class="form-label m-0 mb-3">Image du Profil</label>
                            <div id="image-puppy-profile-<%= puppy.id %>" class="col-6">
                                <% if (puppy.image_path_puppies_profil) { %>
                                  <img src="/uploads/<%= puppy.image_path_puppies_profil %>" class="rounded float-start" alt="profile-image" width="150px" height="150px">
                                <% } else { %>
                                  <img src="https://img.freepik.com/vecteurs-premium/icone-compte-icone-utilisateur-graphiques-vectoriels_292645-552.jpg?w=740" class="rounded float-start" alt="default-profile-image" width="150px" height="150px">
                                <% } %>
                                <div class="col-4 d-flex align-items-center p-3">
                                  <a href="#!" class="d-inline-block bg-primary link-light lh-1 p-2 mt-2 rounded me-2 text-center" data-bs-toggle="modal" data-bs-target="#uploadPuppiesProfileModal-<%= puppy.id %>">
                                    <i class="bi bi-upload"></i>
                                  </a>
                                  <a href="#!" class="d-inline-block bg-danger link-light lh-1 p-2 mt-2 rounded text-center" onclick="deleteProfileImage('<%= puppy.id %>')">
                                    <i class="bi bi-trash"></i>
                                  </a>
                              </div>
                            </div>
                        </div>
                        <div class="form-group pb-2 d-flex justify-content-end border rounded p-2 mt-2">
                            <div>
                                <label for="is_online">Fiche en ligne</label>
                                <div class="form-check form-switch">
                                    <input class="form-check-input" type="checkbox" id="is_online" name="is_online" value="1" <%= puppy.puppy_is_online ? 'checked disabled' : '' %> onchange="toggleOnlineStatus(<%= puppy.id %>)">
                                    <label class="form-check-label <%= puppy.puppy_is_online ? 'online' : 'offline' %>" id="onlineStatusLabel" for="is_online"><%= puppy.puppy_is_online ? 'En ligne' : 'Hors ligne' %></label>
                                </div>
                            </div>
                        </div>
                        <label class="form-label mt-3">Informations du Profil</label>
                        <form id="updateProfileForm" action="/admin/puppies/<%= marriage.id %>/puppyProfile/<%= puppy.id %>" method="POST" class="row gy-3 gy-xxl-4">
                            <div class="col-12 col-md-6">
                                <label for="puppy_name" class="form-label">Nom</label>
                                <input type="text" class="form-control" id="puppy_name" name="puppy_name" maxlength="15" value="<%= puppy.puppy_name %>">
                            </div>

                            <div class="col-12 col-md-6">
                                <label for="sale_status" class="form-label">Statut de Vente</label>
                                <select class="form-control" id="sale_status" name="sale_status" required>
                                    <option value="available_for_reservation" <%= puppy.sale_status === 'available_for_reservation' ? 'selected' : '' %>>Disponible à la réservation</option>
                                    <option value="in_reservation" <%= puppy.sale_status === 'in_reservation' ? 'selected' : '' %>>En cours de réservation</option>
                                    <option value="reserved" <%= puppy.sale_status === 'reserved' ? 'selected' : '' %>>Réservé</option>
                                    <option value="for_sale" <%= puppy.sale_status === 'for_sale' ? 'selected' : '' %>>À vendre</option>
                                    <option value="sold" <%= puppy.sale_status === 'sold' ? 'selected' : '' %>>Vendu</option>
                                </select>
                            </div>

                            <div class="col-12 col-md-6">
                                <label for="puppy_other_names" class="form-label">Autres noms</label>
                                <input type="text" class="form-control" id="puppy_other_names" name="puppy_other_names" placeholder="Ex: Noms avec affixe" value="<%= puppy.puppy_other_names %>">
                            </div>

                            <div class="col-12 col-md-6">
                                <label for="sale_purpose" class="form-label">Vendu pour</label>
                                <select class="form-control" id="sale_purpose" name="sale_purpose" required>
                                    <option value="reproduction" <%= puppy.sale_purpose === 'reproduction' ? 'selected' : '' %>>Reproduction</option>
                                    <option value="company" <%= puppy.sale_purpose === 'company' ? 'selected' : '' %>>Compagnie</option>
                                </select>
                            </div>

                            <div class="col-12 col-md-6">
                                <label for="puppy_color" class="form-label">Couleur</label>
                                <input type="text" class="form-control" id="puppy_color" name="puppy_color" value="<%= puppy.puppy_color %>" required>
                            </div>

                            <div class="col-12 col-md-6">
                                <label for="puppy_gender" class="form-label">Genre</label>
                                <input type="text" class="form-control" id="puppy_gender" name="puppy_gender" value="<%= puppy.puppy_gender %>" readonly>
                            </div>
                            <div class="col-12 col-md-6">
                                <label for="puppy_category" class="form-label">Catégorie</label>
                                <input type="text" class="form-control" id="puppy_category" name="puppy_category" value="<%= puppy.puppy_category %>" readonly>
                            </div>
                            <div class="col-12 col-md-6">
                                <label for="breed_id" class="form-label">Race</label>
                                <select class="form-control" id="breed_id" name="breed_id" required>
                                    <% breeds.forEach(breed => { %>
                                        <option value="<%= breed.id %>" 
                                            <%= puppy.breed_id === breed.id || (puppy.breed_id === null && breed.id === mother.breed_id) ? 'selected' : '' %>>
                                            <%= breed.name %>
                                        </option>
                                    <% }) %>
                                </select>
                            </div>
                            <div class="col-12 col-md-6">
                                <label for="puppy_breed_type" class="form-label">Type</label>
                                <input type="text" class="form-control" id="puppy_breed_type" name="puppy_breed_type" value="<%= puppy.puppy_breed_type %>" placeholder="Ex : Nain, Standard, Miniature" required >
                            </div>
                            <div class="col-12 col-md-6">
                                <label for="puppy_birth_date" class="form-label">Date de Naissance</label>
                                <input type="date" class="form-control" id="puppy_birth_date" name="puppy_birth_date" value="<%= puppy.formatted_puppy_birth_date %>" readonly>
                            </div>
                            <div class="col-12 col-md-6">
                                <label for="country_id" class="form-label">Pays de naissance</label>
                                <select class="form-control" id="country_id" name="country_id" required>
                                    <% countries.forEach(country => { %>
                                        <option value="<%= country.id %>" <%= country.id === puppy.country_id ? 'selected' : '' %>><%= country.name %></option>
                                    <% }) %>
                                </select>
                            </div>
                            <div class="col-12 col-md-6">
                                <label for="puppy_coat_type" class="form-label">Type de Pelage</label>
                                <input type="text" class="form-control" id="puppy_coat_type" name="puppy_coat_type" value="<%= puppy.puppy_coat_type %>" placeholder="Ex : Court, Long, Bouclé" required>
                            </div>

                            <div class="col-12 col-md-6">
                                <label for="puppy_eye_color" class="form-label">Couleur des yeux</label>
                                <input type="text" class="form-control" id="puppy_eye_color" name="puppy_eye_color" value="<%= puppy.puppy_eye_color %>">
                            </div>
                            
                            <div class="col-12 col-md-6">
                                <label for="register_id" class="form-label">Registre</label>
                                <select class="form-control" id="register_id" name="register_id">
                                    <% registers.forEach(register => { %>
                                        <option value="<%= register.id %>" <%= register.id === puppy.register_id ? 'selected' : '' %>><%= register.name %></option>
                                    <% }) %>
                                </select>
                            </div>
                            <div class="col-12 col-md-6">
                                <label for="puppy_price" class="form-label">Prix :</label>
                                <div class="input-group">
                                    <input type="number" class="form-control" id="puppy_price" name="puppy_price" value="<%= puppy.price %>" step="0.01" placeholder="Entrez le prix du chiot" required>
                                    <span class="input-group-text input-group-text-sm">€</span>
                                </div>
                                <span class="small">Laissez 0 sur le prix pour "Nous consulter" en partie public</span>
                            </div>
                            <div class="col-12 col-md-6">
                                <div class="d-flex align-items-center">
                                    <label for="in_breeding" class="form-label mb-0">Fait-il partie de l'élevage ?</label>
                                    <i class="bi bi-question-circle" data-bs-toggle="modal" data-bs-target="#inBreedingModal" style="margin-bottom: 9px; margin-left: 10px;"></i>
                                </div>
                                <select class="form-control mt-2" id="in_breeding" name="in_breeding" required>
                                    <option value="1" <%= puppy.in_breeding == 1 ? 'selected' : '' %>>Oui</option>
                                    <option value="0" <%= puppy.in_breeding == 0 ? 'selected' : '' %>>Non</option>
                                </select>
                            </div>
                            <div class="col-12 col-md-6">
                                <input type="hidden" class="form-control" id="puppy_slug" name="puppy_slug" value="<%= puppy.puppy_name %>">
                            </div>
                                <div id="successEditProfilePuppyMessage" class="alert alert-success mt-3" role="alert" style="display: none;">
                                    Mise à jour du profil réussie
                                </div>
                                <div id="errorEditProfilePuppyMessage" class="alert alert-danger mt-3" role="alert" style="display: none;">
                                    Erreur lors de la mise à jour du profil
                                </div>


                            <div class="col-12 d-flex justify-content-end">
                                <button type="submit" class="btn btn-primary">Valider les Modifications</button>
                            </div>
                        </form>
                    </div>
                </div>
            </div>
            <div class="tab-pane fade" id="email-tab-pane" role="tabpanel" aria-labelledby="email-tab" tabindex="0">
                <form id="textForm">
                    <input type="hidden" name="puppyId" value="<%= puppy.id %>">
                    <input type="hidden" name="marriageId" value="<%= marriage.id %>">

                    <input type="hidden" name="puppyName" value="<%= puppy.puppy_name %>">

                    <label for="description_id" class=" mt-2">Description de <%= puppy.puppy_name %></label>
                    <div class="form-floating mb-3 mt-1">
                        <textarea class="form-control" placeholder="Description" id="descriptionTextarea" name="puppy_description_animals" style="height: 350px" value="<%= puppy.puppy_description_animals %>"><%= puppy.puppy_description_animals %></textarea>
                    </div>
                    <label for="health_tests_id" class=" mt-3">Test(s) de Santé de <%= puppy.puppy_name %></label>
                    <div class="form-floating mb-3 mt-1">
                        <textarea class="form-control" placeholder="Test de Santé" id="healthTestsTextarea" name="puppy_health_tests" style="height: 150px" value="<%= puppy.puppy_health_tests %>"><%= puppy.puppy_health_tests %></textarea>
                    </div>
                    <div class="row">
                        <div id="successMessage" class="alert alert-success mt-3" role="alert" style="display: none;">
                            Mise à jour des textes réussie
                        </div>
                        <div id="errorMessage" class="alert alert-danger mt-3" role="alert" style="display: none;">
                            Erreur lors de la mise à jour des textes du chiot
                        </div>
                        <div class="d-flex justify-content-end">
                            <button type="submit" class="btn btn-primary" id="saveTextsButton">Enregistrer</button>
                        </div>
                    </div>
                </form>
            </div>
            <div class="tab-pane fade" id="password-tab-pane" role="tabpanel" aria-labelledby="password-tab" tabindex="0">

                <div class="container text-center">
                    <div class="d-grid gap-2 mb-4">
                        <button class="btn btn-primary mb-2 uploadPhotosPuppiesBtn" type="button" id="uploadPhotosPuppiesBtn-<%= puppy.id %>" data-photos-puppy-id="<%= puppy.id %>">Télécharger des photos (1620 x 1080)</button>
                    </div>
                    <form id="updatePuppiesPhotosForm-<%= puppy.id %>" action="/<%= marriage.id %>/uploadPuppyPhotos/<%= puppy.id %>" method="POST" enctype="multipart/form-data" data-photos-puppy-id="<%= puppy.id %>" data-marriage-id="<%= marriage.id %>" class="row gy-3 gy-xxl-4" style="display:none;">
                        <input type="file" name="photos" id="PuppyPhotoInput-<%= puppy.id %>" multiple>
                        <input type="hidden" name="puppies_id" id="puppies_id" value="<%= puppy.id %>">
                        <input type="hidden" name="marriage_id" id="marriage_id" value="<%= marriage.id %>">
                        <input type="hidden" name="puppy-gender" id="puppy-gender" value="<%= puppy.gender %>">
                    </form>
                </div>
                <div class="album py-5 bg-body-tertiary min-height-500">
                    <div class="container">                                
                        <div class="row row-cols-1 row-cols-sm-2 g-3" id="PuppyPhotoContainer-<%= puppy.id %>">
                            <% images.forEach(image => { %>
                                <div class="col" id="photo-<%= puppy.id %>-<%= image.id %>">
                                    <div class="card shadow-sm position-relative">
                                        <img src="/uploads/<%= image.image_path %>" class="bd-placeholder-img card-img-top" width="200" height="350" alt="<%= image.balise_alt %>" style="object-fit: cover;">
                                        <button class="btn btn-danger position-absolute top-0 end-0 m-2" data-photo-id="<%= image.id %>" data-photo-path="<%= image.image_path %>" onclick="deletePhoto('<%= puppy.id %>', '<%= image.id %>', '<%= image.image_path %>')">
                                            <i class="bi bi-trash"></i>
                                        </button>
                                    </div>
                                </div>
                            <% }); %>
                        </div>
                    </div>
                </div>
            </div>
            <div class="tab-pane fade" id="description-tab-pane" role="tabpanel" aria-labelledby="description-tab" tabindex="0" >
                <div class="album py-5  min-height-500">
                    <div class="container-fluid">                                
                        <div class="row row-cols-1 row-cols-sm-2 g-3" id="DescriptionPuppyPhotoContainer-<%= puppy.id %>">
                            <% if (images.length > 0) { %>
                                <% images.forEach(image => { %>
                                    <div class="col-xl-6 col-lg-6 col-md-12 col-sm-12 col-xs-12">
                                        <div class="card mb-4 shadow-sm">
                                            <img src="/uploads/<%= image.image_path %>" class="card-img-top object-fit-cover" alt="<%= image.balise_alt %>" width="200" height="350">
                                            <div class="card-body">
                                                <form id="baliseForm" data-id="<%= image.id %>" id="update-alt-form-<%= image.id %>" class="update-alt-form" method="POST" action="/puppies/<%= marriage.id %>/imagesDescription/<%= puppy.id %>" data-marriage-id="<%= marriage.id %>" data-puppy-id="<%= puppy.id %>">
                                                    <div class="form-group">
                                                        <label for="balise_alt_<%= image.id %>" class="d-flex align-items-center mb-2">
                                                            <span>Balise alt</span>
                                                            <span id="badge-<%= image.id %>" class="badge text-bg-success mb-0 ms-2 <%= image.alt_modified ? '' : 'd-none' %>" style="margin-bottom: 10px;">Modifié</span>
                                                        </label>
                                                        <input type="text" class="form-control <%= image.alt_modified ? 'border-success' : 'border-danger' %>" id="balise_alt_<%= image.id %>" name="balise_alt" value="<%= image.balise_alt %>" placeholder="Décrire l'action ou l'animal">
                                                        <input type="hidden" name="imageId" value="<%= image.id %>">
                                                        <input type="hidden" name="balise_title" value="Photos de <%= puppy.puppy_name %>">
                                                        <input type="hidden" name="puppiesId" id="puppiesId" value="<%= puppy.id %>">
                                                        <input type="hidden" name="marriagesId" id="marriagesId" value="<%= marriage.id %>">
                                                    </div>
                                                    <button type="submit" class="btn btn-primary mt-2">Mettre à jour</button>
                                                </form>
                                            </div>
                                        </div>
                                    </div>
                                <% }) %>
                            <% } else { %>
                                <div class=" col-12 justify-content-center align-items-center" style="height: 100vh;">
                                    <div class="alert alert-primary text-center" role="alert">
                                        Aucune image trouvée pour <%= puppy.puppy_name %>
                                    </div>
                                </div>
                            <% } %>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
